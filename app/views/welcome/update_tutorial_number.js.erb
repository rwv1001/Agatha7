<% eval("#{table_name}.set_controller(search_ctl)");  %>  
<% updated_objects = search_ctl.GetUpdateObjects(table_name, ["id"], ids); %>
<% updated_objects.each do |row| %>
    const element = document.getElementById('<%= "#{row.id}_#{table_name}" %>');
    if (element) {
        const newRowHtml = `<%= j render(:partial => 'shared/search_results_row_button', :object => row) %>`;
        const tempContainer = document.createElement('div');
        tempContainer.innerHTML = newRowHtml;
        const newNode = tempContainer.firstElementChild;
        element.replaceWith(newNode);
        setcheck('<%= "#{table_name}_check_#{row.id}" %>', true);
    }
<% end %>

recolour('TutorialSchedule');
action_select_no_js();
if (window.showNotification) {
  window.showNotification('<%= success_str %>', 'success');
} else {
  alert('<%= success_str %>');
}
unwait();