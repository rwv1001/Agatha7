<% if table_name.length > 0 %>
<% new_multi_table = MultiTable.new("#{table_name}", search_ctl) %>
console.log("üîß MULTI_TABLE_CREATE JS EXECUTING - Table: <%= table_name %>");
console.log("Looking for div: multi_change_table_div_<%= table_name %>");
const targetDiv = document.getElementById("<%= "multi_change_table_div_#{table_name}" %>");
