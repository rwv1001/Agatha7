<% table_name = "Tutorial" %>
<% search_ctl = search_ctls[table_name] %>
<% eval("#{table_name}.set_controller(search_ctl)"); %>
<% updated_objects = search_ctl.GetUpdateObjects(table_name, "id", ids); %>
<% updated_objects.each do |row| %>
  {
    const elementId = '<%= "#{row.id}_#{table_name}" %>';
    const replacementHtml = `<%= j render(:partial => 'shared/search_results_row_button', :object => row) %>`;
    const existingElement = document.getElementById(elementId);

    if (existingElement) {
      const tempDiv = document.createElement('div');
      tempDiv.innerHTML = replacementHtml;
      const newElement = tempDiv.firstElementChild;
      existingElement.replaceWith(newElement);
      
      setcheck('<%= "#{table_name}_check_#{row.id}" %>', true);
    }
  }
<% end %>

action_select_no_js();
if (window.showNotification) {
  window.showNotification('<%= "#{success_str}" %>', 'success');
} else {
  alert('<%= "#{success_str}" %>');
}
unwait();
